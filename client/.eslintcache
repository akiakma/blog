...form,\n            [e.target.name]: e.target.value,\n        });\n    };\n\n    const onSubmit = e => {\n        e.preventDefault();\n        const { email, password } = form;\n        const user = { email, password };\n        dispatch({\n            type: LOGIN_REQUEST,\n            payload: user,\n        });\n    };\n    let url = useLocation();\n\n    // here's for badge count\n    const newFeed = useSelector(state => state.badge.count);\n    return (\n        <div>\n            <Header\n                style={{\n                    backgroundColor: \"white\",\n                    position: \"fixed\",\n                    zIndex: \"100\",\n                }}\n                className=\"header\"\n            >\n                <Menu\n                    className=\"header-menu\"\n                    style={{\n                        backgroundColor: \"white\",\n                        color: \"black\",\n                        fontSize: \"1rem\",\n                        border: \"none\",\n                    }}\n                    mode=\"horizontal\"\n                    selectedKeys={[url.pathname]}\n                >\n                    <Menu.Item style={{ borderBottom: \"none\" }} key=\"/\">\n                        <Link\n                            style={{\n                                color: \"black\",\n                            }}\n                            to=\"/\"\n                        >\n                            {url.pathname === \"/\" ? (\n                                <HomeFilled />\n                            ) : (\n                                <HomeOutlined />\n                            )}\n                            Home\n                        </Link>\n                    </Menu.Item>\n                    <Menu.Item style={{ borderBottom: \"none\" }} key=\"/news\">\n                        <Link style={{ color: \"black\" }} to=\"/news\">\n                            {url.pathname === \"/news\" ? (\n                 